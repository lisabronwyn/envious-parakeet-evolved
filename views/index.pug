            h4 Alternate
            .table-wrapper
              table.alt
                thead
                  tr
                    th Name
                    th Description
                    th Price
                tbody
                  tr
                    td Item One
                    td Ante turpis integer aliquet porttitor.
                    td 29.99
                  tr
                    td Item Two
                    td Vis ac commodo adipiscing arcu aliquet.
                    td 19.99
                  tr
                    td Item Three
                    td  Morbi faucibus arcu accumsan lorem.
                    td 29.99
                  tr
                    td Item Four
                    td Vitae integer tempus condimentum.
                    td 19.99
                  tr
                    td Item Five
                    td Ante turpis integer aliquet porttitor.
                    td 29.99
                tfoot
                  tr
                    td(colspan='2')
                    td 100.00
          section
            h3.major Buttons
            ul.actions
              li
                a.button.special(href='#') Special
              li
                a.button(href='#') Default
            ul.actions
              li
                a.button(href='#') Default
              li
                a.button.small(href='#') Small
            ul.actions
              li
                a.button.special.icon.fa-download(href='#') Icon
              li
                a.button.icon.fa-download(href='#') Icon
            ul.actions
              li
                span.button.special.disabled Disabled
              li
                span.button.disabled Disabled
          section
            h3.major Form
            form(method='post', action='#')
              .field.half.first
                label(for='demo-name') Name
                input#demo-name(type='text', name='demo-name', value='', placeholder='THE YES UNIVERSE')
              .field.half
                label(for='demo-email') Email
                input#demo-email(type='email', name='demo-email', value='', placeholder='theyesuniverse@gmail.com')
              .field
                label(for='demo-category') Category
                .select-wrapper
                  select#demo-category(name='demo-category')
                    option(value='') -
                    option(value='1') Manufacturing
                    option(value='1') Shipping
                    option(value='1') Administration
                    option(value='1') Human Resources
              .field.half.first
                input#demo-priority-low(type='radio', name='demo-priority', checked='')
                label(for='demo-priority-low') Low
              .field.half
                input#demo-priority-high(type='radio', name='demo-priority')
                label(for='demo-priority-high') High
              .field.half.first
                input#demo-copy(type='checkbox', name='demo-copy')
                label(for='demo-copy') Email me a copy
              .field.half
                input#demo-human(type='checkbox', name='demo-human', checked='')
                label(for='demo-human') Not a robot
              .field
                label(for='demo-message') Message
                textarea#demo-message(name='demo-message', placeholder='Enter THE YES UNIVERSE', rows='6')
              ul.actions
                li
                  input.special(type='submit', value='Send Message')
                li
                  input(type='reset', value='Reset')
      // Footer
      footer#footer
        p.copyright
          | © Design: 
          a(href='https://www.instagram.com/yesladypheonix/') YOU CANT TOUCH THESE FEATHERS
          | .
